  "panels": [
    {
      "id": 1,
      "title": "Average Temperature",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PDEE91DDB90597936"
          },
          "rawSql": "SELECT window_start as time, avg_temp as value FROM sensor_aggregates WHERE window_start >= NOW() - INTERVAL 1 HOUR ORDER BY time",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {},
          "unit": "celsius"
        }
      }
    },
