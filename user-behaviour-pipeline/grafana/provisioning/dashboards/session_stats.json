{
  "id": null,
  "title": "Session Stats",
  "schemaVersion": 37,
  "version": 1,
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Session Duration",
      "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
      "targets": [
        {
          "refId": "A",
          "rawQuery": true,
          "rawSql": "SELECT toDateTime(sessionStart/1000) AS time, durationMs AS value FROM session_stats ORDER BY time"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ms" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Events per Session",
      "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
      "targets": [
        {
          "refId": "B",
          "rawQuery": true,
          "rawSql": "SELECT toDateTime(sessionStart/1000) AS time, events AS value FROM session_stats ORDER BY time"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    }
  ],
  "time": { "from": "now-1h", "to": "now" }
}
