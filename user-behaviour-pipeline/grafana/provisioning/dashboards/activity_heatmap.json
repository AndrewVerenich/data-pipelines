{
  "id": null,
  "title": "Activity Heatmap",
  "schemaVersion": 37,
  "version": 1,
  "panels": [
    {
      "id": 1,
      "type": "heatmap",
      "title": "User Activity Heatmap",
      "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
      "targets": [
        {
          "refId": "A",
          "rawQuery": true,
          "rawSql": "SELECT toDateTime(windowStart/1000) AS time, key, value FROM activity_heatmap ORDER BY time"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "options": {
        "showValue": "auto",
        "color": { "mode": "scheme", "scheme": "blues" },
        "legend": { "show": true },
        "tooltip": { "mode": "single" }
      }
    }
  ],
  "time": { "from": "now-1h", "to": "now" }
}
